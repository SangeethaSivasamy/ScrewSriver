package com.practiceprograms;

public class BubbleSort {
public static void main(String args[]) {
	int[] a = {1,7,5,6,4,3,2};
	int n=a.length;
	for(int i=0;i<n-1;i++) {
		System.out.println("i--"+i+"a[i]--"+a[i]+"a[i+1]--"+a[i+1]);
		int temp = a[i];
		if(a[i]>a[i+1]) {
			a[i]=a[i+1];
			a[i-1]=temp;
		}
	}
	
	for(int i:a) {
		System.out.println(a[i]);
	}
}
}
